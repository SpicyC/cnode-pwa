{
  "name": "cnode-pwa",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://cnode.surge.sh",
  "devDependencies": {
    "babel-core": "6.22.1",
    "babel-eslint": "7.1.1",
    "babel-jest": "18.0.0",
    "babel-loader": "6.2.10",
    "babel-preset-react-app": "^2.1.1",
    "babel-runtime": "^6.20.0",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "cross-spawn": "4.0.2",
    "css-loader": "0.26.1",
    "detect-port": "1.0.1",
    "dotenv": "2.0.0",
    "duplicate-package-checker-webpack-plugin": "^1.2.0",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "1.6.0",
    "eslint-plugin-flowtype": "2.21.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.10.0",
    "filesize": "3.3.0",
    "fs-extra": "0.30.0",
    "gh-pages": "^0.12.0",
    "gzip-size": "3.0.0",
    "html-webpack-plugin": "2.24.0",
    "http-proxy-middleware": "0.17.3",
    "jest": "18.1.0",
    "json-loader": "0.5.4",
    "object-assign": "4.1.1",
    "postcss-cssnext": "^2.9.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "1.2.2",
    "promise": "7.1.1",
    "react-addons-perf": "^15.4.2",
    "react-dev-utils": "^0.5.1",
    "recursive-readdir": "2.1.1",
    "strip-ansi": "3.0.1",
    "style-loader": "0.13.1",
    "stylefmt": "^5.3.0",
    "stylelint": "^7.9.0",
    "stylelint-config-standard": "^16.0.0",
    "sw-precache": "^5.1.0",
    "url-loader": "0.5.7",
    "webpack": "1.14.0",
    "webpack-bundle-analyzer": "^2.3.1",
    "webpack-dev-server": "1.16.2",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "2.0.2",
    "why-did-you-update": "^0.0.8"
  },
  "dependencies": {
    "github-markdown-css": "^2.4.1",
    "lodash": "^4.17.4",
    "material-ui": "^0.17.0",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-helmet": "^5.0.0-beta",
    "react-overdrive": "^0.0.1",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "react-router-scroll": "^0.4.1",
    "react-tap-event-plugin": "^2.0.1",
    "recompose": "^0.22.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "timeago-react": "^1.0.8"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js && sw-precache --config=config/sw-precache-config.js",
    "deploy": "gh-pages -d build",
    "test": "eslint src/**/*.js",
    "test-jest": "node scripts/test.js --env=jsdom"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb",
    "env": {
      "browser": true
    },
    "rules": {
      "semi": [
        "error",
        "never"
      ],
      "react/jsx-filename-extension": "off",
      "react/no-danger": "off"
    }
  },
  "stylelint": {
    "extends": "stylelint-config-standard"
  }
}
